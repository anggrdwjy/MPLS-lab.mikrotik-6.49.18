[admin@CE1] > export
# oct/18/2025 11:43:20 by RouterOS 6.49.19
# software id = 
#
#
#
/interface bridge
add name=lo0
/interface ethernet
set [ find default-name=ether1 ] comment=PE5 mtu=9000
set [ find default-name=ether2 ] comment=CE2 mtu=9000
/interface wireless security-profiles
set [ find default=yes ] supplicant-identity=MikroTik
/routing ospf area
add area-id=0.0.0.10 name=area10
/routing ospf instance
set [ find default=yes ] name=ospf100 router-id=192.168.10.2
/ip address
add address=192.168.10.2 comment=CE1 interface=lo0 network=192.168.10.2
add address=172.16.20.82/30 comment=PE5 interface=ether1 network=172.16.20.80
add address=172.16.20.85/30 comment=CE2 interface=ether2 network=172.16.20.84
/ip dhcp-client
add disabled=no interface=ether1
/routing ospf interface
add cost=65000 interface=lo0 network-type=point-to-point passive=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s \
    interface=ether1 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s \
    interface=ether2 network-type=point-to-point use-bfd=yes
/routing ospf network
add area=area10 network=192.168.10.2/32
add area=area10 network=172.16.20.80/30
add area=area10 network=172.16.20.84/30
/system identity
set name=CE1


[admin@PE8] > export
# oct/18/2025 07:47:02 by RouterOS 6.49.19
# software id = 
#
#
#
/interface bridge
add name=lo0
/interface ethernet
set [ find default-name=ether1 ] comment=PE4 mtu=9000
set [ find default-name=ether2 ] comment=PE4 mtu=9000
set [ find default-name=ether3 ] comment=PE7 mtu=9000
set [ find default-name=ether4 ] comment=PE7 mtu=9000
set [ find default-name=ether5 ] comment=CE6 mtu=9000
/interface wireless security-profiles
set [ find default=yes ] supplicant-identity=MikroTik
/routing ospf area
add area-id=0.0.0.20 name=area20
/routing ospf instance
set [ find default=yes ] name=ospf100 router-id=192.168.150.5
/ip address
add address=192.168.150.5 comment=PE8 interface=lo0 network=192.168.150.5
add address=172.16.20.78/30 comment=PE4 interface=ether1 network=172.16.20.76
add address=172.16.20.74/30 comment=PE4 interface=ether2 network=172.16.20.72
add address=172.16.20.42/30 comment=PE7 interface=ether3 network=172.16.20.40
add address=172.16.20.46/30 comment=PE7 interface=ether4 network=172.16.20.44
add address=172.16.20.113/30 comment=CE6 interface=ether5 network=172.16.20.112
/routing ospf interface
add cost=65000 interface=lo0 network-type=point-to-point passive=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s interface=ether1 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s interface=ether2 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s interface=ether3 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s interface=ether4 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s interface=ether5 network-type=point-to-point use-bfd=yes
/routing ospf network
add area=backbone network=192.168.150.5/32
add area=backbone network=172.16.20.72/30
add area=backbone network=172.16.20.76/30
add area=backbone network=172.16.20.40/30
add area=backbone network=172.16.20.44/30
add area=area20 network=172.16.20.112/30
/system identity
set name=PE8

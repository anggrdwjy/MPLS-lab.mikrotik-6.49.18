# oct/18/2025 07:47:32 by RouterOS 6.49.19
# software id = 
#
#
#
/interface bridge
add name=lo0
/interface ethernet
set [ find default-name=ether1 ] comment=PE3 mtu=9000
set [ find default-name=ether2 ] comment=PE3 mtu=9000
set [ find default-name=ether3 ] comment=PE8 mtu=9000
set [ find default-name=ether4 ] comment=PE8 mtu=9000
set [ find default-name=ether5 ] comment=PE6 mtu=9000
set [ find default-name=ether6 ] comment=PE6 mtu=9000
set [ find default-name=ether7 ] comment=CE4 mtu=9000
set [ find default-name=ether8 ] comment=CE5 mtu=9000
/interface wireless security-profiles
set [ find default=yes ] supplicant-identity=MikroTik
/routing ospf area
add area-id=0.0.0.20 name=area20
add area-id=0.0.0.40 name=area40
/routing ospf instance
set [ find default=yes ] name=ospf100 router-id=192.168.150.6
/ip address
add address=192.168.150.6 comment=PE7 interface=lo0 network=192.168.150.6
add address=172.16.20.70/30 comment=PE3 interface=ether1 network=172.16.20.68
add address=172.16.20.66/30 comment=PE3 interface=ether2 network=172.16.20.64
add address=172.16.20.41/30 comment=PE8 interface=ether3 network=172.16.20.40
add address=172.16.20.45/30 comment=PE8 interface=ether4 network=172.16.20.44
add address=172.16.20.34/30 comment=PE6 interface=ether5 network=172.16.20.32
add address=172.16.20.38/30 comment=PE6 interface=ether6 network=172.16.20.36
add address=172.16.20.101/30 comment=CE4 interface=ether7 network=\
    172.16.20.100
add address=172.16.20.105/30 comment=CE5 interface=ether8 network=\
    172.16.20.104
/routing ospf interface
add cost=65000 interface=lo0 network-type=point-to-point passive=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=\
    10s hello-interval=5s interface=ether1 network-type=point-to-point \
    use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=\
    10s hello-interval=5s interface=ether2 network-type=point-to-point \
    use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=\
    10s hello-interval=5s interface=ether3 network-type=point-to-point \
    use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=\
    10s hello-interval=5s interface=ether4 network-type=point-to-point \
    use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=\
    10s hello-interval=5s interface=ether5 network-type=point-to-point \
    use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=\
    10s hello-interval=5s interface=ether6 network-type=point-to-point \
    use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=\
    10s hello-interval=5s interface=ether7 network-type=point-to-point \
    use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=\
    10s hello-interval=5s interface=ether8 network-type=point-to-point \
    use-bfd=yes
/routing ospf network
add area=backbone network=192.168.150.6/32
add area=backbone network=172.16.20.64/30
add area=backbone network=172.16.20.68/30
add area=backbone network=172.16.20.40/30
add area=backbone network=172.16.20.44/30
add area=backbone network=172.16.20.32/30
add area=backbone network=172.16.20.36/30
add area=area40 network=172.16.20.100/30
add area=area20 network=172.16.20.104/30
/system identity
set name=PE7

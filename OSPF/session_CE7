[admin@CE7] > export
# oct/18/2025 11:44:12 by RouterOS 6.49.19
# software id = 
#
#
#
/interface bridge
add name=lo0
/interface ethernet
set [ find default-name=ether1 ] comment=PE4 mtu=9000
set [ find default-name=ether2 ] comment=CE8 mtu=9000
/interface wireless security-profiles
set [ find default=yes ] supplicant-identity=MikroTik
/routing ospf area
add area-id=0.0.0.50 name=area50
/routing ospf instance
set [ find default=yes ] name=ospf100 router-id=192.168.10.8
/ip address
add address=192.168.10.8 comment=CE7 interface=lo0 network=192.168.10.8
add address=172.16.20.122/30 comment=CE8 interface=ether2 network=172.16.20.120
add address=172.16.20.118/30 comment=PE4 interface=ether1 network=172.16.20.116
/ip dhcp-client
add disabled=no interface=ether1
/routing ospf interface
add cost=65000 interface=lo0 network-type=point-to-point passive=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s \
    interface=ether1 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s \
    interface=ether2 network-type=point-to-point use-bfd=yes
/routing ospf network
add area=area50 network=172.16.20.116/30
add area=area50 network=172.16.20.120/30
add area=area50 network=192.168.10.8/32
/system identity
set name=CE7

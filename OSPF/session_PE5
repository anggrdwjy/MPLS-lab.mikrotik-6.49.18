# oct/18/2025 07:48:38 by RouterOS 6.49.19
# software id = 
#
#
#
/interface bridge
add name=lo0
/interface ethernet
set [ find default-name=ether1 ] comment=PE1 mtu=9000
set [ find default-name=ether2 ] comment=PE1 mtu=9000
set [ find default-name=ether3 ] comment=PE6 mtu=9000
set [ find default-name=ether4 ] comment=PE6 mtu=9000
set [ find default-name=ether5 ] comment=CE1 mtu=9000
/interface wireless security-profiles
set [ find default=yes ] supplicant-identity=MikroTik
/routing ospf area
add area-id=0.0.0.10 name=area10
/routing ospf instance
set [ find default=yes ] name=ospf100 router-id=192.168.150.8
/ip address
add address=192.168.150.8 comment=PE5 interface=lo0 network=192.168.150.8
add address=172.16.20.54/30 comment=PE1 interface=ether1 network=172.16.20.52
add address=172.16.20.50/30 comment=PE1 interface=ether2 network=172.16.20.48
add address=172.16.20.25/30 comment=PE6 interface=ether3 network=172.16.20.24
add address=172.16.20.29/30 comment=PE6 interface=ether4 network=172.16.20.28
add address=172.16.20.81/30 comment=CE1 interface=ether5 network=172.16.20.80
/routing ospf interface
add cost=65000 interface=lo0 network-type=point-to-point passive=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s interface=ether1 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s interface=ether2 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s interface=ether3 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s interface=ether4 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s interface=ether5 network-type=point-to-point use-bfd=yes
/routing ospf network
add area=backbone network=192.168.150.8/32
add area=backbone network=172.16.20.24/30
add area=backbone network=172.16.20.28/30
add area=backbone network=172.16.20.52/30
add area=backbone network=172.16.20.48/30
add area=area10 network=172.16.20.80/30
/system identity
set name=PE5

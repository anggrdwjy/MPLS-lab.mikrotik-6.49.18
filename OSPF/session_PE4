# oct/18/2025 11:42:09 by RouterOS 6.49.19
# software id = 
#
#
#
/interface bridge
add name=lo0
/interface ethernet
set [ find default-name=ether1 ] comment=PE8 mtu=9000
set [ find default-name=ether2 ] comment=PE8 mtu=9000
set [ find default-name=ether3 ] comment=PE3 mtu=9000
set [ find default-name=ether4 ] comment=PE3 mtu=9000
set [ find default-name=ether5 ] comment=CE7 mtu=9000
/interface wireless security-profiles
set [ find default=yes ] supplicant-identity=MikroTik
/routing ospf area
add area-id=0.0.0.50 name=area50
/routing ospf instance
set [ find default=yes ] name=ospf100 router-id=192.168.150.4
/ip address
add address=192.168.150.4 comment=PE4 interface=lo0 network=192.168.150.4
add address=172.16.20.77/30 comment=PE8 interface=ether1 network=172.16.20.76
add address=172.16.20.73/30 comment=PE8 interface=ether2 network=172.16.20.72
add address=172.16.20.18/30 comment=PE3 interface=ether3 network=172.16.20.16
add address=172.16.20.22/30 comment=PE3 interface=ether4 network=172.16.20.20
add address=172.16.20.117/30 comment=CE12 interface=ether5 network=172.16.20.116
/ip dhcp-client
add disabled=no interface=ether1
/routing ospf interface
add cost=65000 interface=lo0 network-type=point-to-point passive=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s \
    interface=ether1 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s \
    interface=ether2 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s \
    interface=ether3 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s \
    interface=ether4 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s \
    interface=ether5 network-type=point-to-point use-bfd=yes
/routing ospf network
add area=backbone network=192.168.150.4/32
add area=backbone network=172.16.20.72/30
add area=backbone network=172.16.20.76/30
add area=backbone network=172.16.20.16/30
add area=backbone network=172.16.20.20/30
add area=area50 network=172.16.20.116/30
/system identity
set name=PE4

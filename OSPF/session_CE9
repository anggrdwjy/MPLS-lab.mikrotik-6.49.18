[admin@CE9] > export
# oct/18/2025 07:56:47 by RouterOS 6.49.19
# software id = 
#
#
#
/interface bridge
add name=lo0
/interface ethernet
set [ find default-name=ether1 ] comment=PE3 mtu=9000
set [ find default-name=ether2 ] comment=CE10 mtu=9000
/interface wireless security-profiles
set [ find default=yes ] supplicant-identity=MikroTik
/routing ospf area
add area-id=0.0.0.30 name=area30
/routing ospf instance
set [ find default=yes ] name=ospf100 router-id=192.168.10.10
/ip address
add address=192.168.10.10 comment=CE9 interface=lo0 network=192.168.10.10
add address=172.16.20.130/30 comment=PE3 interface=ether1 network=\
    172.16.20.128
add address=172.16.20.134/30 comment=CE10 interface=ether2 network=\
    172.16.20.132
/ip dhcp-client
add disabled=no interface=ether1
/routing ospf interface
add cost=65000 interface=lo0 network-type=point-to-point passive=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=\
    10s hello-interval=5s interface=ether1 network-type=point-to-point \
    use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=\
    10s hello-interval=5s interface=ether2 network-type=point-to-point \
    use-bfd=yes
/routing ospf network
add area=area30 network=192.168.10.10/32
add area=area30 network=172.16.20.132/30
add area=area30 network=172.16.20.128/30
/system identity
set name=CE9

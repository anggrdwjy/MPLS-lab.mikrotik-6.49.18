[admin@CE11] > export
# oct/18/2025 07:57:49 by RouterOS 6.49.19
# software id = 
#
#
#
/interface bridge
add name=lo0
/interface ethernet
set [ find default-name=ether1 ] comment=PE2 mtu=9000
set [ find default-name=ether2 ] comment=CE12 mtu=9000
/interface wireless security-profiles
set [ find default=yes ] supplicant-identity=MikroTik
/routing ospf area
add area-id=0.0.0.60 name=area60
/routing ospf instance
set [ find default=yes ] name=ospf100 router-id=192.168.10.12
/ip address
add address=192.168.10.12 comment=CE11 interface=lo0 network=192.168.10.12
add address=172.16.20.142/30 comment=PE2 interface=ether1 network=\
    172.16.20.140
add address=172.16.20.146/30 comment=CE12 interface=ether2 network=\
    172.16.20.144
/ip dhcp-client
add disabled=no interface=ether1
/routing ospf interface
add cost=65000 interface=lo0 network-type=point-to-point passive=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=\
    10s hello-interval=5s interface=ether1 network-type=point-to-point \
    use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=\
    10s hello-interval=5s interface=ether2 network-type=point-to-point \
    use-bfd=yes
/routing ospf network
add area=area60 network=172.16.20.140/30
add area=area60 network=172.16.20.144/30
add area=area60 network=192.168.10.12/32
/system identity
set name=CE11

/interface bridge
add name=lo0
/interface ethernet
set [ find default-name=ether1 ] comment=PE6 mtu=9000
set [ find default-name=ether2 ] comment=PE6 mtu=9000
set [ find default-name=ether3 ] comment=PE1 mtu=9000
set [ find default-name=ether4 ] comment=PE1 mtu=9000
set [ find default-name=ether5 ] comment=PE3 mtu=9000
set [ find default-name=ether6 ] comment=PE3 mtu=9000
set [ find default-name=ether7 ] comment=CE10 mtu=9000
set [ find default-name=ether8 ] comment=CE11 mtu=9000
/interface wireless security-profiles
set [ find default=yes ] supplicant-identity=MikroTik
/routing ospf area
add area-id=0.0.0.60 name=area60
add area-id=0.0.0.30 name=area30
/routing ospf instance
set [ find default=yes ] name=ospf100 router-id=192.168.150.2
/ip address
add address=192.168.150.2 comment=PE2 interface=lo0 network=192.168.150.2
add address=172.16.20.61/30 comment=PE6 interface=ether1 network=172.16.20.60
add address=172.16.20.57/30 comment=PE6 interface=ether2 network=172.16.20.56
add address=172.16.20.2/30 comment=PE1 interface=ether3 network=172.16.20.0
add address=172.16.20.6/30 comment=PE1 interface=ether4 network=172.16.20.4
add address=172.16.20.9/30 comment=PE3 interface=ether5 network=172.16.20.8
add address=172.16.20.13/30 comment=PE3 interface=ether6 network=172.16.20.12
add address=172.16.20.137/30 comment=CE10 interface=ether7 network=172.16.20.136
add address=172.16.20.141/30 comment=CE11 interface=ether8 network=172.16.20.140
/ip dhcp-client
add disabled=no interface=ether1
/routing ospf interface
add cost=65000 interface=lo0 network-type=point-to-point passive=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s \
    interface=ether1 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s \
    interface=ether2 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s \
    interface=ether3 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s \
    interface=ether4 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s \
    interface=ether5 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s \
    interface=ether6 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s \
    interface=ether7 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s \
    interface=ether8 network-type=point-to-point use-bfd=yes
/routing ospf network
add area=backbone network=192.168.150.2/32
add area=backbone network=172.16.20.0/30

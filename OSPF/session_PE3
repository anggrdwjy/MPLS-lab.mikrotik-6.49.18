/interface bridge
add name=lo0
/interface ethernet
set [ find default-name=ether1 ] comment=PE7 mtu=9000
set [ find default-name=ether2 ] comment=PE7 mtu=9000
set [ find default-name=ether3 ] comment=PE4 mtu=9000
set [ find default-name=ether4 ] comment=PE4 mtu=9000
set [ find default-name=ether5 ] comment=PE2 mtu=9000
set [ find default-name=ether6 ] comment=PE2 mtu=9000
set [ find default-name=ether7 ] comment=CE8 mtu=9000
set [ find default-name=ether8 ] comment=CE9 mtu=9000
/interface wireless security-profiles
set [ find default=yes ] supplicant-identity=MikroTik
/routing ospf area
add area-id=0.0.0.50 name=area50
add area-id=0.0.0.30 name=area30
/routing ospf instance
set [ find default=yes ] name=ospf100 router-id=192.168.150.3
/ip address
add address=192.168.150.3 comment=PE3 interface=lo0 network=192.168.150.3
add address=172.16.20.69/30 comment=PE7 interface=ether1 network=172.16.20.68
add address=172.16.20.65/30 comment=PE7 interface=ether2 network=172.16.20.64
add address=172.16.20.17/30 comment=PE4 interface=ether3 network=172.16.20.16
add address=172.16.20.21/30 comment=PE4 interface=ether4 network=172.16.20.20
add address=172.16.20.10/30 comment=PE2 interface=ether5 network=172.16.20.8
add address=172.16.20.14/30 comment=PE2 interface=ether6 network=172.16.20.12
add address=172.16.20.125/30 comment=CE8 interface=ether7 network=172.16.20.124
add address=172.16.20.129/30 comment=CE9 interface=ether8 network=172.16.20.128
/ip dhcp-client
add disabled=no interface=ether1
/routing ospf interface
add cost=65000 interface=lo0 network-type=point-to-point passive=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s \
    interface=ether1 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s \
    interface=ether2 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s \
    interface=ether3 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s \
    interface=ether4 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s \
    interface=ether5 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s \
    interface=ether6 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s \
    interface=ether7 network-type=point-to-point use-bfd=yes
add authentication=md5 authentication-key=multimedia123 cost=1 dead-interval=10s hello-interval=5s \
    interface=ether8 network-type=point-to-point use-bfd=yes
/routing ospf network
add area=backbone network=192.168.150.3/32
add area=backbone network=172.16.20.8/30
